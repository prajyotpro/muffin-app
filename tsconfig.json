{
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "allowJs": true,
    "target": "es6",
    "outDir": ".build",
    "moduleResolution": "node",
    "rootDir": "./",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/utils/*": ["app/utils/*"],
      "@/controller/*": ["app/controller/*"],
      "@/repository/*": ["app/repository/*"],
      "@/ioc/*": ["app/ioc/*"],
      "@/model/*": ["app/model/*"],
      "@/usecase/*": ["app/usecase/*"],
      "@/core/*": ["app/core/*"],
      "@/test/*": ["test/*"],
      "@/service/*": ["app/service/*"],
    }
  }
}
